{
	"name": "uxmz/w3-total-cache",
	"description": "W3 Total Cache",
	"type": "wordpress-plugin",
	"homepage": "https://wordpress.org/plugins/w3-total-cache/",
	"authors": [
		{
			"name": "BoldGrid",
			"homepage": "https://wordpress.org/plugins/w3-total-cache"
		},
		{
			"name": "UX Information Technologies",
			"homepage": "https://ux.co.mz",
			"email": "developers@ux.co.mz"
		}
	],
    "require": {
        "aws/aws-sdk-php": "3.183.8",
        "guzzlehttp/guzzle": "6.*|7.*",
        "symfony/polyfill-intl-idn" : "1.20.*",
        "symfony/polyfill-intl-normalizer" : "1.20.*",
        "symfony/polyfill-mbstring" : "1.20.*",
        "aws/aws-php-sns-message-validator": "^1.8"
    },
	"require-dev": {
        "yoast/phpunit-polyfills": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "dev-develop"
    },
    "replace": {
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-mbstring": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
